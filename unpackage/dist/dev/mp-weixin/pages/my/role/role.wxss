
.deng {
	width: 100%;
}
.deng>view {
	width: 80%;
	margin: auto;
	line-height: 2rem;
	border-radius: 5rem;
	background-color: #fc4369;
	color: white;
	padding: 0 1rem;
	box-sizing: border-box;
	text-align: center;
	margin-bottom: 1rem;
}
.back {
	height: 100vh;
	background: url(data:image/jpeg;base64,) no-repeat;
	overflow-y: hidden;
	overflow-x: hidden;
	width: 100%;
	background: url(data:image/jpeg;base64,) no-repeat;
	background-size: 100% auto;
	padding-top: 13%;
}

