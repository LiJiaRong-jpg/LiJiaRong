
#di>view>text:last-child {
	float: right;
}
#di>view>text:first-child,
#di>view>text:nth-child(2) {
	float: left;
}
#di>view {
	width: 100%;
	clear: both;
	padding: 1rem 0;
	border-bottom: 1px #555555 solid;
	height: 1rem;
}
#di>view>text:first-child {
	margin-left: 1rem;
}
.order-section {
	width: 100%;
	height: 4rem;
	background-color: white;
	margin-top: 2rem;
	padding-top: 1rem;
}
.order-section>view {
	width: 25%;
	float: left;
}

